{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true,
          "tsx": true,
          "ts": true,
        "js": true
        }
        
    }, 
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-expressions": "warn",
        "no-unused-vars": "warn"
    }
}
